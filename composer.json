{
    "name": "elcweb/keyvaluestore-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily access and set key-value",
    "license": "MIT",
    "authors": [
        {
            "name": "Etienne Lachance",
            "email": "el@elcweb.ca"
        }
    ],
    "require": {
        "php": ">=5.4.1",
        "symfony/symfony": ">=2.3,<3.0-dev",
        "doctrine/dbal": "<2.5",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "51systems/doctrine-encrypt": "~4.0",
        "jms/serializer-bundle": "dev-master",
        "stof/doctrine-extensions-bundle": "dev-master",
        "elcweb/common-bundle": "dev-master",
        "elcweb/doctrine-encrypt-bundle": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "symfony/finder": ">=2.7,<3.0-dev"
    },
    "suggest": {
        "doctrine/doctrine-migrations-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Elcweb\\KeyValueStoreBundle": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "target-dir": "Elcweb/KeyValueStoreBundle"
}
